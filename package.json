        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.1.0",
        "@modelcontextprotocol/sdk": "^1.0.4",
        "ansi-escapes": "^7.0.0",
        "chalk": "^5.3.0",
