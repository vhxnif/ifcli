{
  "name": "ifcli",
  "version": "v0.1.0",
  "module": "index.ts",
  "description": "ai chat cli & some tools command",
  "keywords": ["deepseek", "ai", "bun", "chat cli"],
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/vhxnif/ifcli#README.adoc",
  "bin": {
    "tools": "./build/command.js",
    "chat": "./build/chat-command.js"
  },
  "scripts": {
    "build-tools": "bun build ./src/command.ts --outdir ./build --target bun",
    "build-chat": "bun build ./src/chat-command.ts --outdir ./build --target bun",
    "tools": "bun run ./src/command.ts --",
    "chat": "bun run ./src/chat-command.ts --"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@eslint/js": "^9.15.0",
    "@types/bun": "^1.1.9",
    "@types/lodash": "^4.17.7",
    "bun-types": "^1.1.28",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.15.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "openai": "^4.62.1",
    "table": "^6.8.2"
  }
}