{
  "name": "@vhxnif/ifcli",
  "version": "0.1.3",
  "description": "ai chat cli & some tools command",
  "keywords": [
    "deepseek",
    "ai",
    "bun",
    "chat cli"
  ],
  "license": "MIT",
  "type": "module",
  "author": "vhxnif",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vhxnif/ifcli.git"
  },
  "files": [
    "build",
    "README.adoc"
  ],
  "engines": {
    "bun": ">=1.1.36"
  },
  "bin": {
    "ifts": "build/command.js",
    "ifct": "build/chat-command.js"
  },
  "scripts": {
    "build:tools": "bun build ./src/command.ts --outdir ./build --target bun --minify",
    "build:chat": "bun build ./src/chat-command.ts --outdir ./build --target bun --minify",
    "build:all": "npm run build:tools && npm run build:chat",
    "tools": "bun run ./src/command.ts --",
    "chat": "bun run ./src/chat-command.ts --"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@eslint/js": "^9.15.0",
    "@types/bun": "^1.1.9",
    "@types/lodash": "^4.17.7",
    "bun-types": "^1.1.28",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.15.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nanoid": "^5.0.7",
    "openai": "^4.62.1",
    "table": "^6.8.2"
  }
}
